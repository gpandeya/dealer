<h1>Cars</h1>

<div>
    
    <%= link_to  'New', new_car_path %>
</div>
<table>
    <thead>
        <th>VIN</th>
        <th>Make</th>
        <th>Model</th>
        <th>Year</th>
    </thead>
    <tbody>
        <% @cars.each do |c| %>
        <tr>
            <td><%= link_to c.vin,c %></td>
            <td><%= c.make %></td>
            <td><%= c.model %></td>
            <td><%= c.year %></td>
        </tr>
        
        
        <% end %>
        
    </tbody>
    
</table>